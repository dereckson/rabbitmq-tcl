{
    "linters": {
        "chmod": {
            "type": "chmod"
        },
        "clang-format": {
            "type": "clang-format",
            "include": [
                "(\\.c$)",
                "(\\.h$)",
                "(\\.m$)",
                "(\\.mm$)"
            ],
            "exclude": [
                "(vendor/)"
            ]
        },
        "filename": {
            "type": "filename"
        },
        "generated": {
            "type": "generated"
        },
        "json": {
            "type": "json",
            "include": [
                "(^\\.arcconfig$)",
                "(^\\.arclint$)",
                "(\\.json$)"
            ]
        }
    }
}
